/* input */
input[type="text"], input[type="password"], select, textarea { width:98%; border:1px #CCC solid; padding:10px 0px !important; padding-left:1% !important; padding-right:1% !important; }
select { padding:10.5px 5px !important;}
input[type="submit"], input[type="button"] { padding:8px 15px; cursor:pointer; }
input[type="radio"], input[type="checkbox"] { vertical-align:-3px; }

input[type="text"]:focus, input[type="password"]:focus, select:focus, textarea:focus, input[type="radio"]:focus, input[type="checkbox"]:focus, input[type="submit"]:focus, input[type="button"]:focus { outline:none; }

.validation-error { background:#fbd2d2 !important;  border:1px #ff5c5c solid !important; }
.validation-ok { background:#e9ffd4 !important; border:1px #7dd12b solid !important; }

.input-icon-width { width:90% !important; }
