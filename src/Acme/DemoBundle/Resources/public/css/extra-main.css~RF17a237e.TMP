/* errow- */
[class^=errow-] { padding:4px 10px; color:#FFF; background:red; position:relative; float:right;  }
[class^=errow-] div { width:0px; height:0px; border-top:6px solid transparent; border-bottom:6px solid transparent; border-right:6px solid red; position:absolute; margin-left:-14px; }

/* arrow */
.right-arrow { position:relative; float:right; margin-right:-10px; width:0px; height:0px; margin-top:-40px;

 border-left:10px #ececec solid;  border-top:10px solid transparent;  border-bottom:10px solid transparent;

 }

